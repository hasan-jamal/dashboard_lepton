    &gt;Active toggle link&lt;/a
  &gt;
  &lt;a
    href=&#34;#&#34;
    class=&#34;btn btn-primary disabled&#34;
    tabindex=&#34;-1&#34;
    aria-disabled=&#34;true&#34;
