      &lt;/select&gt;
    &lt;/div&gt;
  &lt;/div&gt;

  &lt;div class=&#34;input-group multiple mb-2&#34;&gt;
    &lt;div class=&#34;flex-grow-1&#34;&gt;
      &lt;select class=&#34;lpx_select2&#34;&gt;
        &lt;option value=&#34;1&#34;&gt;Option 1&lt;/option&gt;
        &lt;option value=&#34;2&#34;&gt;Option 2&lt;/option&gt;
        &lt;option value=&#34;3&#34;&gt;Option 3&lt;/option&gt;
      &lt;/select&gt;
    &lt;/div&gt;
    &lt;span class=&#34;input-group-text&#34; style=&#34;border-left: 0 !important&#34;&gt; .00&lt;/span&gt;
  &lt;/div&gt;

  &lt;div class=&#34;input-group multiple mb-2&#34;&gt;
    &lt;div class=&#34;flex-grow-1&#34;&gt;
      &lt;select class=&#34;lpx_select2&#34;&gt;
        &lt;option value=&#34;1&#34;&gt;Option 1&lt;/option&gt;
        &lt;option value=&#34;2&#34;&gt;Option 2&lt;/option&gt;
        &lt;option value=&#34;3&#34;&gt;Option 3&lt;/option&gt;
      &lt;/select&gt;
    &lt;/div&gt;
    &lt;button class=&#34;btn btn-secondary&#34; type=&#34;button&#34; id=&#34;button-addon2&#34;&gt;
      Button
    &lt;/button&gt;
  &lt;/div&gt;
&lt;/div&gt;
 </code></pre>
                </div>
            </div>
        
            <div class="tab-pane fade " id="codeTab20221124141424865464" role="tabpanel" aria-labelledby="codeTab20221124141424865464-tab">
                <div class="highlight-prev">
                    <div class="button-area">
                        <button class="copy-button">Copy</button>
                    </div>
                    <pre><code class="language-js" style="height:857px;">$(&#34;.lpx_select2&#34;).select2({
  theme: &#34;bootstrap-5&#34;,
  placeholder: &#34;Select a option&#34;,
}),
  $(&#34;.lpx_select2_1&#34;).select2({
    theme: &#34;bootstrap-5&#34;,
    placeholder: &#34;Select a option&#34;,
    maximumSelectionLength: 2,
  }),
  $(&#34;.lpx_select2_2&#34;).select2({
    theme: &#34;bootstrap-5&#34;,
    placeholder: &#34;Select a option&#34;,
    minimumResultsForSearch: Infinity,
  }),
  $(&#34;.lpx_select2_3&#34;).select2({
    theme: &#34;bootstrap-5&#34;,
    placeholder: &#34;Select a option&#34;,
    allowClear: true,
  });
 </code></pre>
                </div>
