<script>
  document.addEventListener('DOMContentLoaded', function () {
      var qcard = document.querySelector('.questions-search'); 
      var qcardbody  = document.querySelector('.questions-search .card');  

      qcard.addEventListener('mouseenter', function (e) {
          qcardbody.classList.add('show-big-search');
      });
      qcard.addEventListener('mouseleave', function () {
          qcardbody.classList.remove('show-big-search');
      }) 
  })
</script>  
