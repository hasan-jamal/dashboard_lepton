<script type="text/javascript">
    (function () {
        leptonx.LanguageSettingEvent.on(function (event) {
            // handle language change here
            console.log(event);
        });
    })();

    (function () {
        document.addEventListener('DOMContentLoaded', function () {
            leptonx.init.run(); // init leptonx
        });
    })();
